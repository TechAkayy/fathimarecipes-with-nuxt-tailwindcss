    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.10.6",
    "@unocss/nuxt": "^65.4.0",
    "@unocss/preset-icons": "^0.62.3",
    "@vueuse/nuxt": "^12.4.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.15.1",
    "nuxt-icon": "^0.6.10",
    "postcss": "^8.5.0",
    "prettier": "^3.4.2",
    "rehype-external-links": "^3.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"

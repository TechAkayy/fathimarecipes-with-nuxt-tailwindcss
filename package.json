{
  "name": "fathimarecipes-with-nuxt-tailwindcss",
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://fathimarecipes-with-nuxt-tailwindcss.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/pinegrow/fathimarecipes-with-nuxt-tailwindcss.git"
  },
  "scripts": {
    "dev": "npm-run-all --serial remote:tw:cls generate:types dev-only",
    "dev-only": "nuxt dev",
    "now": "npm run build && npm run preview",
    "lighthouse": "npx unlighthouse --no-cache --site https://fathimarecipes-with-nuxt-tailwindcss.netlify.app",
    "build": "npm-run-all --serial remote:tw:cls build-only",
    "build-only": "nuxt build",
    "start": "node .output/server/index.mjs",
    "remote:tw:cls": "node scripts/remote-classes.js",
    "generate:types": "node ./scripts/generate-types.js",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "analyze": "nuxt analyze",
    "lint": "npm run lint:fix && npm run format",
    "lint:fix": "eslint --fix ./",
    "format": "prettier --write \"**/*.{html,vue,svelte,astro,js,jsx,cjs,mjs,ts,tsx,cts,mts,css,md,mdx,json}\" !public --ignore-path .gitignore"
  },
  "dependencies": {
    "@directus/sdk": "^20.2.0",
    "@directus/types": "^13.4.0",
    "@directus/visual-editing": "^1.1.0",

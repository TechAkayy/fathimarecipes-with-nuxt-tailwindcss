    "@vee-validate/nuxt": "^4.13.2",
    "@vueuse/core": "^11.0.3",
    "directus-sdk-typegen": "^0.2.1",
    "dotenv": "^17.2.3",
    "lite-youtube-embed": "^0.3.3",
    "pinia": "^2.2.2"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.295",
    "@nuxt/content": "^2.13.4",
    "@nuxt/eslint": "^0.7.5",
